<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="account_analytic_line_extend_form_view" model="ir.ui.view">
            <field name="name">account_analytic_line_extend_form_view</field>
            <field name="model">account.analytic.line</field>
            <field name="inherit_id" ref="analytic.view_account_analytic_line_form"/>
            <field name="arch" type="xml">
                <xpath expr="//form[1]/sheet[1]/group[1]/group[not(@name)][1]" position="inside">
                    <group name="stock picking" string="Stock picking">
                        <field name="stock_picking_line_id"/>
                    </group>
                </xpath>
            </field>
        </record>
    </data>
</odoo>