<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="purchase_requisition_custom.product_product_tree_inherit" model="ir.ui.view">
            <field name="name">purchase_requisition_custom.product_product_tree_inherit</field>
            <field name="model">product.product</field>
            <field name="inherit_id" ref="product.product_product_tree_view"/>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='price']" position="after">
<!--                    <field name="available_stock" optional="show"/>-->
                </xpath>
            </field>
        </record>
    </data>
</odoo>